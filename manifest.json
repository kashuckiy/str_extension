{
    "name": "STR Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "qa_logo.png"
    },
    "content_scripts": [
      {
        "js": ["popup.js"],
        "matches": [
          "<all_urls>"
        ],
        "run_at": "document_end"
      }
    ],
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
      "<all_urls>"
  ]
  }